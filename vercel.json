{
    "version": 2,
    "builds": [
      {
        "src": "Backend/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "Al-Chat-App/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "Backend/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "Al-Chat-App/dist/$1"
      }
    ]
  }